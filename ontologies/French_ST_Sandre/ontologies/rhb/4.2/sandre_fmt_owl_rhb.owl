    </AnnotationAssertion>
    <AnnotationAssertion>
        <AnnotationProperty abbreviatedIRI="rdfs:label"/>
        <IRI>http://id.eaufrance.fr/ddd/scm/2.0/DispoCptMigrPoiss</IRI>
