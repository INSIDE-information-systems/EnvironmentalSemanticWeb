                <DatatypeRestriction>
            <Datatype abbreviatedIRI="xsd:string"/>
            <FacetRestriction facet="http://www.w3.org/2001/XMLSchema#maxLength">
                <Literal datatypeIRI="http://www.w3.org/2001/XMLSchema#integer">255</Literal>
            </FacetRestriction>
            </DatatypeRestriction>        
        
    </DataPropertyRange>




